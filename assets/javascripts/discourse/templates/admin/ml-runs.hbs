<table class='table ml'>
  <thead>
    <tr>
      <th>{{i18n 'ml.admin.run.label'}}</th>
      <th>{{i18n 'ml.admin.model.label'}}</th>
      <th>{{i18n 'ml.admin.dataset.label'}}</th>
      <th>{{i18n 'ml.admin.run.accuracy'}}</th>
      <th>{{i18n 'ml.admin.status'}}</th>
      <th>{{i18n 'ml.admin.actions'}}</th>
      {{#if activeLabel}}
        {{d-button action="clearFilter" class="btn-primary" label="admin.logs.staff_actions.clear_filters" icon="cross"}}
      {{/if}}
    </tr>
  </thead>
  
  <tbody>
    {{#each visibleRuns as |r|}}
      {{ml-run run=r}}
    {{else}}
      {{i18n 'search.no_results'}}
    {{/each}}
  </tbody>
</table>
