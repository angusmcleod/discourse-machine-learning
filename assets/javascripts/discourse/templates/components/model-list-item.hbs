<div class="col value first model-label">
  {{model.label}}
</div>
<div class="col value model-image">
  {{model.namespace}}
</div>
<div class="col value model-run">
  {{model.run}}
</div>
<div class="col value model-data">
  {{model.data}}
</div>
<div class="col value model-status">
  {{status}}
</div>
<div class="col value model-actions">
  {{#if built}}
    {{d-button action="removeModelImage" actionParam=model class="btn btn-small" label="ml.admin.models.remove"}}
  {{else}}
    {{d-button action="buildModelImage" actionParam=model class="btn btn-small" label="ml.admin.models.build" disabled=building}}
  {{/if}}
</div>
<div class="clearfix"></div>
